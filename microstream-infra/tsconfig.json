{
    "compilerOptions": {
        "strict": true,
        "outDir": "bin",
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "pretty": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "src/**/*.ts",
    ],
      "exclude": [
        "node_modules"
    ]
}
